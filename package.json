{
  "name": "kabla",
  "version": "1.2.0",
  "description": "monitor of web visibility",
  "keywords": [
    "visibility"
  ],
  "author": "Bouziane Amine",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "prettier": "^2.8.7",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tsup": "^7.1.0",
    "typescript": "^5.0.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "tsup src/* --format cjs,esm --dts",
    "release": "pnpm run build && changeset publish",
    "lint": "tsc"
  },
  "files": [
    "dist/**/*"
  ],
  "homepage": "https://kabla-app.vercel.app"
}
